{
  "name": "@vl/api",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "prebuild": "yarn run clean",
    "build": "webpack --config webpack.prod.ts --display-error-details --bail",
    "prestart": "yarn run clean",
    "start": "webpack --config webpack.dev.ts --display-error-details --bail",
    "prestart:prod": "rimraf dist && yarn run build",
    "start:prod": "node dist/server.js",
    "clean": "rimraf dist",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json --detectOpenHandles"
  },
  "dependencies": {
    "@nestjs/common": "^5.5.0",
    "@nestjs/core": "^5.5.0",
    "@nestjs/elasticsearch": "^6.0.1",
    "@nestjs/jwt": "^0.2.1",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/typeorm": "^5.2.2",
    "@types/elasticsearch": "^5.0.32",
    "@vl/utils": "1.0.0",
    "aws-sdk": "^2.389.0",
    "axios": "^0.18.0",
    "bignumber.js": "^8.0.2",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "elasticsearch": "^16.0.0",
    "ipfs": "^0.35.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "moment-timezone": "^0.5.23",
    "mysql": "^2.16.0",
    "node-jose": "^1.1.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "readline-sync": "^1.4.9",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.9",
    "typescript": "^3.0.1",
    "web3": "^1.0.0-beta.55",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.7.2",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^23.3.1",
